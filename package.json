    "@azure/identity": "^4.10.1",
    "@azure/storage-blob": "^12.27.0",
    "@google-cloud/storage": "^7.16.0",
    "@nichoth/backblaze-b2": "^1.7.1",
    "minio": "^8.0.5",
    "glob": "^11.0.3",
    "rimraf": "^6.0.1"
